     %echo Generating a basic OpenPGP key
     Key-Type: DSA
     Key-Length: 1024
     Subkey-Type: ELG-E
     Subkey-Length: 1024
     Name-Real: Joe Tester
     Name-Comment: with stupid passphrase
     Name-Email: test@tester.com
     Expire-Date: 0
     Passphrase: abc123
     %pubring gpg-test-key.pub
     # Do a commit here, so that we can later print "done" :-)
     %commit
     %echo done
